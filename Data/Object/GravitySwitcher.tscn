const AICONS := [preload(\"res://Sprites/Particles/ArrowUp2.png\"), preload(\"res://Sprites/Particles/ArrowDown1.png\")]

var y := 0.0
var _time := 0.0

func _ready():
	set_as_toplevel(true)
	
	yield(get_tree(), \"idle_frame\")
	global_position = get_parent().global_position
	
func _process(delta):
	update()
	
	if !get_tree().get_root().get_node(\"Main\").editing: return
	
	_time += delta
	
	if !get_parent().mode:
		y = abs(sin(_time * 2)) * 4
	else:
		y = -abs(sin(_time * 2)) * 4
	
func _draw():
	if get_tree().get_root().get_node(\"Main\").editing:
		draw_texture(AICONS[get_parent().mode], -AICONS[get_parent().mode].get_size() / 2 + Vector2(0, y))
"

